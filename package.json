{
  "name": "justchatting",
  "version": "0.1.0",
  "description": "Create groups and chatting",
  "main": "app.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production node server/app.js",
    "server": "nodemon server/app.js",
    "client": "npm run serve --prefix client",
    "client:install": "npm install --prefix client",
    "client:build": "npm run build --prefix client",
    "dev": "cross-env NODE_ENV=development concurrently \"npm run server\" \"npm run client\""
  },
  "author": "ZipLant",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.2.0",
    "cross-env": "^7.0.2",
    "nodemon": "^2.0.3"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "bcryptjs": "^2.4.3",
    "config": "^3.3.1",
    "express": "^4.17.1",
    "express-validator": "^6.4.0",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^2.4.2",
    "multer": "^1.4.2",
    "sequelize": "^6.3.4",
    "socket.io": "^2.3.0"
  }
}
